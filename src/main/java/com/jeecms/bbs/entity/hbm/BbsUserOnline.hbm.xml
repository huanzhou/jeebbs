<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.jeecms.bbs.entity">
	<class
		name="BbsUserOnline"
		table="bbs_user_online"
	>
		<meta attribute="sync-DAO">false</meta>
		<cache usage="read-write"/>
		<id name="id" type="java.lang.Integer" column="user_id">
			<generator class="foreign"><param name="property">user</param></generator>
		</id>

		<property
			name="onlineLatest"
			column="online_latest"
			type="java.lang.Double"
			not-null="false"
			length="10"
		/>
		<property
			name="onlineDay"
			column="online_day"
			type="java.lang.Double"
			not-null="false"
			length="10"
		/>
		<property
			name="onlineWeek"
			column="online_week"
			type="java.lang.Double"
			not-null="false"
			length="10"
		/>
		<property
			name="onlineMonth"
			column="online_month"
			type="java.lang.Double"
			not-null="false"
			length="10"
		/>
		<property
			name="onlineYear"
			column="online_year"
			type="java.lang.Double"
			not-null="false"
			length="10"
		/>
		<property
			name="onlineTotal"
			column="online_total"
			type="java.lang.Double"
			not-null="false"
			length="10"
		/>
		<one-to-one name="user" class="BbsUser" constrained="true"/>

	</class>
</hibernate-mapping>