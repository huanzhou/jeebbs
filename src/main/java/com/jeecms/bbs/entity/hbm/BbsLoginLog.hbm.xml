<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.jeecms.bbs.entity">
	<class
		name="BbsLoginLog"
		table="bbs_login_log"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="integer"
			column="id"
		>
			<generator class="native"/>
		</id>

		<property
			name="loginTime"
			column="login_time"
			type="timestamp"
			not-null="false"
			length="10"
		/>
		<property
			name="logoutTime"
			column="logout_time"
			type="timestamp"
			not-null="false"
			length="10"
		/>
		<property
			name="ip"
			column="ip"
			type="string"
			not-null="false"
			length="255"
		/>
		<many-to-one
			name="user"
			column="user_id"
			class="BbsUser"
			not-null="false"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>